<prefigure xmlns="https://prefigure.org"
             label="sinusoidal-forcing-4">

<diagram dimensions="(300,300)" margins="25">
  <definition>b = 2</definition>
  <definition>k = 17</definition>
 <definition>f(t,y) = (y[1],-k*y[0] - b*y[1] - 2*sin(3*t))</definition>
  <coordinates bbox="[-1.5,-7,2.5,4]">
    <grid-axes xlabel="x(t)" ylabel="y(t)"/>

    <plot-de-solution function="f" t0="0" t1="10" 
          y0="(2,2)" axes="(y0,y1)" arrow="yes"   N="600"/>


  </coordinates>

      
    <annotations>
      <annotation ref="figure"
      text="Phase plane for x double prime plus 2 times x prime plus 17 times x equals minus 2 times sine of 3 times t with initial conditions x of 0 equal to 2 and x prime of 0 equal to 2."/>
    </annotations>
  


</diagram>
</prefigure>
