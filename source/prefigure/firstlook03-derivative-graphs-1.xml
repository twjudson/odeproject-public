<prefigure xmlns="https://prefigure.org"
             label="derivative-graphs-1">
  <diagram dimensions="(300,300)" margins="5">
    <definition>f(x) = (x + 2)*(x - 2)</definition>
    <coordinates bbox="[-3,-4.5,3,6]">
      <grid-axes xlabel="y" ylabel="f(y)"/>

      <graph function='f' />
    
    </coordinates>

    <annotations>
      <annotation ref="figure"
        text="A quadratic polynomial with roots at -2 and 2 that opens up"/>
    </annotations>
  
  </diagram>
</prefigure>
  
