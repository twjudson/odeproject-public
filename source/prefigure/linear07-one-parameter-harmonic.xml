<prefigure xmlns="https://prefigure.org"
             label="one-parameter-harmonic">

<diagram dimensions="(300,250)" margins="(35,30,50,30)">
  <coordinates bbox="(-4, -2, 4, 4)">
    <axes decorations="no">
      <xlabel><m>T</m></xlabel>
      <ylabel><m>D</m></ylabel>
    </axes>

    <definition>f(x)=x^2/4</definition>
    <graph function="f"/>

      <label anchor="(0,2)" scale="0.8">
            <it>Undamped</it>
      </label>

      <label anchor="(-2,3)" scale="0.8">
            <it>Under Damped</it>
      </label>

      <label anchor="(-3,0.8)" scale="0.8">
            <it>Over Damped</it>
      </label>

      <label anchor="(4,4)" alignment="n">
            <m>D = T^2/4</m>
      </label>

      <label anchor="(-3,2)" scale="0.8">
            <it>Critically Damped</it>
      </label>

  </coordinates>

      <annotations>
      <annotation ref="figure"
            text="A parabola D equals T squared divided by 4 in the TD plane.  On the D axis above the parabola are undamped oscillators, above the parabola in the second quadrant are under damped oscillators.  Over damped oscillators are in the second quadrant below the parabola.  Critically damped oscillators are on the parabola in the second quadrant."/>
    </annotations>
    


</diagram>

</prefigure>

