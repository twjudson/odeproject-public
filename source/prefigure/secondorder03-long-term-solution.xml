<prefigure xmlns="https://prefigure.org"
             label="long-term-solution">

<diagram dimensions="(300,300)" margins="35">
  <definition>A = 1/sqrt(145)</definition>
  <definition>B = atan(-12)</definition>

  <definition>f(t) = A*cos(2*t + B - pi/2)</definition>
  <coordinates bbox="[0,-0.1,5*pi,0.1]">
    <grid-axes xlabel="t" ylabel="x(t)" h-pi-format="yes"/>

  <graph function="f"/>

  </coordinates>

    
    <annotations>
      <annotation ref="figure"
      text="Integral curve for x double prime plus 6 times x prime plus 5 times x equals sine of 2 times t."/>
    </annotations>
    
</diagram>
</prefigure>


