<prefigure xmlns="https://prefigure.org"
             label="tmd">
<diagram dimensions="(600,200)" margins="20">
  <coordinates bbox="(-2,-2,20,9)">
    <line endpoints="((-1,0),(18,0))"/>

    <line endpoints="((8,-2),(8,8))" dash="6 6" stroke="gray" thickness="1"/>
    <line endpoints="((16,1),(16,8))" dash="6 6" stroke="gray" thickness="1"/>

    <rectangle center="(0,4)" dimensions="(3,8)"
	       fill="lightblue" stroke="black"/>
    <rectangle center="(8,2.5)" dimensions="(4,5)"
	       fill="lightblue" stroke="black"/>
    <rectangle center="(16,1.5)" dimensions="(3,3)"
	       fill="lightblue" stroke="black"/>

    <path start="(1.5,1.2)" stroke="black" thickness="3">
      <lineto point="(6,1.2)" 
	      decoration="coil; number=6 ; dimensions=(6,6)"/>
    </path>

    <path start="(10,1.2)" stroke="black" thickness="3">
      <lineto point="(14.5,1.2)" 
	      decoration="coil; number=6 ; dimensions=(6,6)"/>
    </path>


    <line endpoints="((1.5,3.25),(6,3.25))"/>
    <line endpoints="((10,2.5),(14.5,2.5))"/>

    <line endpoints="((-1,0),(18,0))"/>

    <label anchor="(0,4)"><m>\text{wall}</m></label>

    <label anchor="(8,2.5)"><m>m_1</m></label>
    <label anchor="(16,1.5)"><m>m_2</m></label>

    <label anchor="(8,-1.5)" alignment="e"><m>\rho_1 \cos( \omega t)</m></label>
    <line endpoints="((8,-1.5),(6,-1.5))" stroke="black" thickness="2" arrows="1"/>

    <line endpoints="((8,6.5),(6,6.5))" stroke="black" thickness="2" arrows="1"/>
    <label anchor="(8,6.5)" alignment="e"><m>x_1</m></label>

   <line endpoints="((16,6.5),(14,6.5))" stroke="black" thickness="2" arrows="1"/>
    <label anchor="(16,6.5)" alignment="e"><m>x_2</m></label>

    <rectangle center="(12.25,2.5)" dimensions="(2,0.75)"
	       fill="lightblue" stroke="black"/>

    <rectangle center="(3.75,3.25)" dimensions="(2,0.75)"
	       fill="lightblue" stroke="black"/>

    <label anchor="(2,0.55)" alignment="e"><m>k_1</m></label>
    <label anchor="(10.5,0.55)" alignment="e"><m>k_2</m></label>
    <label anchor="(3.5,4.4)" alignment="e"><m>b_1</m></label>
    <label anchor="(11.9,3.8)" alignment="e"><m>b_2</m></label>

  </coordinates>

  <annotations>
    <annotation ref="figure"
      text="A mass m1 attached by a springs to a wall and a second mass m2 which is attached by a spring to the mass m1.  Both masses slide back and forth on a frictionless surface.  there are dashpots b1 between m1 and the wall and b2 between m1 and m2."/>
  </annotations>
  
</diagram>
</prefigure>


